{
  "name": "comfyui",
  "version": "1.0.0",
  "description": "ComfyUI workflow generation and execution",
  "skillFile": "comfyui.py",
  "configSchema": {
    "server_url": {
      "type": "string",
      "description": "ComfyUI server URL",
      "default": "https://wp08.unicorn.org.cn:40000"
