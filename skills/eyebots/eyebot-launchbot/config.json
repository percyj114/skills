{
  "agent_id": "launchbot",
  "name": "LaunchBot",
  "version": "1.0.0",
  "description": "Full token launch automation and orchestration",
  "enabled": true,
  "triggers": {
    "keywords": [
      "launch token",
      "full launch",
      "one-click launch",
      "stealth launch",
      "launchbot",
      "fair launch",
      "token launch",
      "deploy and launch",
      "bundle launch",
      "coordinated launch",
      "go live"
    ],
    "commands": ["/launch", "/launchbot", "/stealth", "/fairlaunch"],
    "regex_patterns": [
      "launch.*token",
      "deploy.*and.*launch",
      "one.*click.*launch",
      "stealth.*launch"
    ]
  },
  "pricing": {
    "payment_wallet": "0x4A9583c6B09154bD88dEE64F5249df0C5EC99Cf9",
    "accepted_tokens": ["ETH"],
    "plans": {
      "trial": {
        "duration_days": 2,
        "price_eth": 0.027,
        "display": "LaunchBot: 0.027 ETH (2-day) | 5 ETH (lifetime)",
        "features": ["standard_launch", "single_chain", "basic_lp"]
      },
      "lifetime": {
        "duration_days": -1,
        "price_eth": 5.0,
        "display": "LaunchBot: 0.027 ETH (2-day) | 5 ETH (lifetime)",
        "features": ["all_launch_types", "all_chains", "bundle_launch", "stealth", "lp_lock", "social_integration"]
      }
    }
  },
  "chains": {
    "ethereum": {
      "chain_id": 1,
      "enabled": true,
      "rpc_env": "ETH_RPC_URL",
      "dex": "uniswap_v2",
      "locker": "unicrypt"
    },
    "base": {
      "chain_id": 8453,
      "enabled": true,
      "rpc_env": "BASE_RPC_URL",
      "dex": "uniswap_v3",
      "locker": "uncx"
    },
    "polygon": {
      "chain_id": 137,
      "enabled": true,
      "rpc_env": "POLYGON_RPC_URL",
      "dex": "quickswap",
      "locker": "unicrypt"
    },
    "arbitrum": {
      "chain_id": 42161,
      "enabled": true,
      "rpc_env": "ARBITRUM_RPC_URL",
      "dex": "uniswap_v3",
      "locker": "uncx"
    }
  },
  "launch_types": {
    "standard": {
      "steps": ["deploy", "add_liquidity", "verify", "enable_trading"],
      "anti_bot": false,
      "delay_seconds": 0
    },
    "stealth": {
      "steps": ["deploy", "add_liquidity", "delay", "enable_trading", "verify"],
      "anti_bot": true,
      "delay_seconds": 30,
      "max_delay_seconds": 300
    },
    "fair": {
      "steps": ["deploy_fair", "add_liquidity", "verify"],
      "anti_bot": false,
      "no_team_tokens": true,
      "immediate_trading": true
    },
    "bundle": {
      "steps": ["deploy", "add_liquidity", "bundle_buy", "enable_trading", "verify"],
      "anti_bot": true,
      "max_wallets": 10
    }
  },
  "lp_lockers": {
    "unicrypt": {
      "name": "Unicrypt",
      "url": "https://unicrypt.network",
      "supported_chains": [1, 137]
    },
    "uncx": {
      "name": "UNCX Network",
      "url": "https://uncx.network",
      "supported_chains": [1, 8453, 42161]
    },
    "team_finance": {
      "name": "Team Finance",
      "url": "https://team.finance",
      "supported_chains": [1, 137, 42161]
    }
  },
  "automation": {
    "auto_verify": true,
    "auto_lock_lp": true,
    "simulation_before_launch": true,
    "gas_estimation": true
  },
  "limits": {
    "min_lp_eth": 0.1,
    "max_bundle_wallets": 10,
    "max_delay_seconds": 300,
    "min_lock_days": 30
  },
  "social_integration": {
    "twitter": true,
    "telegram": true,
    "discord": true
  }
}
