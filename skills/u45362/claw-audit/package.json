{
  "name": "claw-audit",
  "version": "0.1.0",
  "description": "Security scanner & hardening skill for OpenClaw. Scans skills for malware, audits config, calculates security score.",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "openclaw",
    "clawdbot",
    "moltbot",
    "security",
    "scanner",
    "clawhub",
    "skill",
    "agent-security"
  ],
  "author": "u45362",
  "scripts": {
    "integrity": "node scripts/check-integrity.mjs",
    "integrity:baseline": "node scripts/check-integrity.mjs --baseline",
    "integrity:json": "node scripts/check-integrity.mjs --json",
    "audit:system": "node scripts/audit-system.mjs",
    "audit:system:json": "node scripts/audit-system.mjs --json",
    "audit:system:fix": "node scripts/audit-system.mjs --fix",
    "scan": "bash scripts/scan-skills.sh",
    "scan:json": "bash scripts/scan-skills.sh --json",
    "audit": "node scripts/audit-config.mjs",
    "audit:json": "node scripts/audit-config.mjs --json",
    "audit:fix": "node scripts/audit-config.mjs --fix",
    "score": "node scripts/calculate-score.mjs",
    "score:json": "node scripts/calculate-score.mjs --json",
    "fix": "node scripts/auto-fix.mjs",
    "fix:dry": "node scripts/auto-fix.mjs --dry-run",
    "watch": "node scripts/watch.mjs",
    "test": "bash tests/run.sh",
    "test:verbose": "node --test --test-reporter=spec tests/*.test.mjs",
    "test:tap": "node --test tests/*.test.mjs"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "SKILL.md",
    "clawhub.json",
    "scripts/",
    "references/",
    "README.md",
    "LICENSE"
  ]
}
