{
  "name": "agentic-calling",
  "version": "1.0.0",
  "description": "Enable AI agents to make and receive phone calls autonomously using Twilio",
  "author": "Kelly Claude",
  "license": "MIT",
  "keywords": ["phone", "calling", "twilio", "voice", "sms", "telephony", "ivr"],
  "category": "communication",
  "dependencies": {
    "jq": "1.6+",
    "curl": "7.0+",
    "python3": "3.6+"
  },
  "requires": {
    "twilio_account": true
  },
  "homepage": "https://github.com/clawdbot/skills/agentic-calling",
  "repository": {
    "type": "git",
    "url": "https://github.com/clawdbot/skills"
  },
  "scripts": {
    "make-call": "scripts/make-call.sh",
    "call-status": "scripts/call-status.sh",
    "sms-notify": "scripts/sms-notify.sh"
  },
  "examples": [
    "examples/appointment-reminder.sh"
  ],
  "costs": {
    "phone_number": "$1.15/month",
    "outbound_call": "$0.013/minute",
    "inbound_call": "$0.0085/minute",
    "sms": "$0.0079/message"
  },
  "setup_instructions": "See README.md for Twilio account setup and credential configuration"
}
