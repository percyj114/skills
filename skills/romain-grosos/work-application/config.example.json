{
  "allow_write": false,
  "allow_export": true,
  "allow_scrape": false,
  "allow_tracking": true,
  "default_template": "classic",
  "default_color": "#2563eb",
  "default_lang": "fr",
  "readonly_mode": false,
  "report_mode": "analysis",
  "storage": {
    "backend": "local",
    "path": "/OpenClaw/work-application"
  },
  "scraper": {
    "searches": [
      {
        "name": "DevOps CDI WTTJ",
        "platform": "wttj",
        "params": {
          "query": "devops",
          "location": "Paris, France",
          "radius": 30
        }
      },
      {
        "name": "DevOps Freelance",
        "platform": "free-work",
        "params": {
          "query": "devops",
          "contracts": "contractor"
        }
      }
    ],
    "filters": {
      "excludeCompanies": [],
      "minTJM": 400,
      "minSalary": 45000,
      "maxAge": 7,
      "locations": [],
      "excludeLocations": []
    },
    "rate_limit_ms": 2000,
    "scroll_timeout_ms": 5000
  }
}
