  "endpoints": {
    "api": "https://agentshield.live/api",
    "registry": "https://agentshield.live/registry",
    "verify": "https://agentshield.live/verify",
    "docs": "https://github.com/bartelmost/agentshield"
  },
  "installation": {
    "clawhub": "clawhub install agentshield-audit",
