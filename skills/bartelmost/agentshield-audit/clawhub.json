  "license": "MIT",
  "homepage": "https://agentshield.live",
  "repository": "https://github.com/bartelmost/agentshield",
  "tags": [
    "security",
    "audit",
    "privacy",
    "registry",
    "certificates",
    "ed25519",
    "trust",
    "compliance",
    "gdpr",
    "eu-ai-act"
  ],
  "features": [
    "52+ local security tests",
    "Ed25519 identity certificates",
    "Public trust registry",
    "CRL revocation support",
    "EU AI Act compliance",
    "Zero data exfiltration",
    "Challenge-response protocol",
    "Tamper-proof PDF reports"
